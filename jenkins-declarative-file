pipeline{
	agent any
	tools{
	  maven "maven3.9.9"
	}
	stages{
	  stage("clone from scm"){
	    steps{
	    sh "echo clone from scm"
	    git branch: 'jan99', url: 'https://github.com/githubih/liontech-joinedproject.git'
	    }
	  }   
      
      stage("maven build"){
        steps{
        sh "running a unit test on source code"
        sh "maven is packaging the source code"
        sh "mvn install"
        sh "mvn verify"
        sh "mvn clean package"
      } 
    } 


      	}
}