pipeline{
	agent any
	tools{
	  maven "maven3.9.9"
	}
	stages{
	  stage("clone from scm"){
	    steps{
	    sh "echo jenkins to clone from scm"
	    git branch: 'jan10', url: 'https://github.com/githubih/liontech-joinedproject.git'
	    }
	  }

      stage("maven build"){
	    steps{
	    sh "echo running unit test on source code"
	    sh "echo maven is packaging the source code"
	    sh "mvn verify"
	    sh "mvn validate"
	    sh "mvn install"
	    sh "mvn clean package"
	  }
    }

	  stage("code quality analysis"){
        steps{
        sh "sonarqube code quality checks on source code"
        sh "mvn sonar:sonar"
        }
      }
	}
	}